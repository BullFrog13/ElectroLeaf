{
  "name": "electro-leaf1",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "author": "Vadym Harkusha, Yevgeniy Tkachenko",
  "description": "Desktop application for managing Nanoleaf devices",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "react-build": "react-scripts build",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "run-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js -c.mac.identity=null",
    "pack": "electron-builder --dir",
    "electron-build": "electron-builder",
    "lint": "eslint ./src --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "appId": "electro.leaf",
    "extends": null,
    "productName": "ElectroLeaf",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.9.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "body-parser": "1.19.0",
    "classnames": "^2.2.6",
    "clsx": "^1.1.0",
    "color-convert": "2.0.1",
    "compression": "1.7.4",
    "concurrently": "^5.0.2",
    "ejs": "3.0.1",
    "electron-is-dev": "^1.1.0",
    "electron-osx-sign": "^0.4.15",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "material-ui-color-picker": "^3.5.0",
    "method-override": "3.0.0",
    "morgan": "1.9.1",
    "nanoleaf-client": "^0.2.2",
    "nanoleaf-layout": "^3.0.3",
    "react": "^16.12.0",
    "react-color": "2.18.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.1",
    "wait-on": "^3.3.0"
  },
  "devDependencies": {
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0"
  }
}
